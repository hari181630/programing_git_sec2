package helloUser;
import java.util.Scanner;

public class UserInputScanner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int factorial=1;
		for(int i=1;i<=n;i++) {
			factorial*=i;
			
		}
		System.out.println(factorial);
}
}