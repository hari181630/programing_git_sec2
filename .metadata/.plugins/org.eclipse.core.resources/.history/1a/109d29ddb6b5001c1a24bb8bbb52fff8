package loops;

public class Arrays {
	//Array is a data structure 
	//Array is a simiar type of data which size is fixed 
	//we can retrieve and sort data efficiently
	int x[]= {11,12,13,12,15,18,20};
public static void main(String[] args) {
	Arrays a1 = new Arrays();
	int x[]= {11,12,13,12,15,18,20};
	int size = x.length;
	System.out.println(size);
	System.out.println(size-1);
}
}
