package loops;

public class Arrays {
	//Array is a data structure 
	//Array is a simiar type of data which size is fixed 
	//we can retrieve and sort data efficiently
	int x[]= {11,12,13,12,15,13,18,18,20};
public static void main(String[] args) {
	Arrays a1 = new Arrays();
	int y[]= {12,13,12,15,13,18,18,11,20};
	int count=0;
//	int size = x.length;
//	System.out.println(size);
	for(int i=0;i<=y.length;i++) {
		for(int j=i+1;j<=y.length;j++) {
			if(y[j]<y[i]) {
				
				int h=y[i];
				y[i]=y[j];
				y[j]=h;
			System.out.println(y[i]);
			}
		}
		}
	
	}

}
