import java.util.Scanner;

public class SimpleFactorial {
	public static void main(String[] args) {
		
	
	Scanner scn =new Scanner(System.in);
	System.out.println("enter first Number:");
	int n=scn.nextInt();
	scn.close();
	int fact=1;
	for(int i=1;i<=n;i++) {
		fact=fact*i;
		System.out.println(fact);
	}
	}
}
