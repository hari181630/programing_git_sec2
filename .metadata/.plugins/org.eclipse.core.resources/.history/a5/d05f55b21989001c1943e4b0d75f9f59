package pkg1;

import java.util.Scanner;

public class CountNoDigit {
public static void main(String[] args) {
	Scanner scn = new Scanner(System.in);
	int temp=scn.nextInt();
	int count=0;
	for(int i=0;i<=temp;i++) {
//		int rem=temp%10;
//		count=count+rem;
		temp++;
		count++;
	}
	System.out.println(count);
}
}
