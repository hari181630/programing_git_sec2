package pkg1;

public class A {
public static void main(String[] args) {
		System.out.println("wap to print all natural no from 1 to n using while loop");
		int i=1;
		while(i<=10) {
		System.out.println(i);
			i++;
			//System.out.println(i);
		}
		int j=10;
		while(j>=1) {
			System.out.println(j);
			j--;
		}
		printAtoZ();
	
	
}
public static void printAtoZ() {
	int A=65;
	while(A<=97) {
		System.out.println(A);
		A++;
	}
}
}
