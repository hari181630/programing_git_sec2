package pkg1;

public class example_method_with_return_Type {

	public static void main(String[] args) {
		int num1=10;
		int num2=addition(10,20);
		int num3=mul(2,4);
	}
	public static int addition(int a,int b) {
		int c= a+b;
		return c;
	}
	public static int mul(int n1,int n2) {
		int n3=n1* n2;
		return n3;
	}

}
