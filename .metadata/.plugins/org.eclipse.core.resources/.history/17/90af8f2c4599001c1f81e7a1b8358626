package collation;

import java.util.ArrayList;

public class Example_Collection {
public static void main(String[] args) {
	ArrayList al = new ArrayList();
	al.add("Hari");//insertion order is maintain and type of index is zero
	al.add(1);//all types of data stored
	al.add('B');//default capacity is 10 
	al.add("Hari");//duplicate data allow
	al.add(null);//calcute capacity=(capacity*1.5)+1
	al.add(null);//null  multiple value are allowed
	System.out.println(al);//[Hari, 1, B, Hari, null, null]
	System.out.println(al.size());
	
	
}
}
