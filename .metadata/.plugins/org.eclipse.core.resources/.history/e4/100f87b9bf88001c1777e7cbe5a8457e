package pkg1;

public class Table {
public static void main(String[] args) {
	showTable(1);
	showTable(2);
	showTable(3);
	
}
public static void showTable(int x)
{
     //write here table logics 
	    for(int i=1; i<=10;i++)
		{
	    	System.out.println(i*x);
		   //System.out.printf("%d X %d = %d \n",i,x,i*x);
		}
}
}
