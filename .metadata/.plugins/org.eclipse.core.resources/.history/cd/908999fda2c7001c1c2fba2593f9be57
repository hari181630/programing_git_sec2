


package String;

public class S1 {

	public static void main(String[] args) {
		String s1="india";
		char y[]=s1.toCharArray();
		int size=y.length;
		char a[]=new char[size];
		int i=0;
		while(i!=size)
		{
			a[size-1-i]=y[i];
			i++;
		}
		System.out.println(s1);
		System.out.println(a);

	}

}
